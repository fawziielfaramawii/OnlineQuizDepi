@model IEnumerable<StudentAttemptDto>

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
    <link href="~/css/AdminViews.css" rel="stylesheet" />
</head>


<body>
    <!-- Main content area -->
    <div id="main-content">
        <section id="instructors">
            <h2>Students Attempet</h2>

            <a class="btn-unban" href="/Instructor/QuizOfInstructor">Back To DashBoard</a>
            <br />
            <br />
            <!-- Add search box to filter instructors -->

            <table id="instructor-table">
                <thead>
                    <tr>
                        <th>Name</th>

                        <th>Phone</th>
                        <th>Age</th>
                        <th>Email</th>
                        <th>score</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var quiz in Model)
                    {
                        <tr>
                            <td>@quiz.UserName</td>
                            <td>@quiz.PhoneNumber</td>
                            <td>@quiz.Age</td>
                            <td>@quiz.Email</td>
                            <td>@quiz.Score</td>


                        </tr>
                    }
                </tbody>
            </table>
        </section>
    </div>

    <script>
        // Function to filter instructors by name
        function filterInstructors() {
            // Get the search input field
            var input = document.getElementById("searchInput");
            var filter = input.value.toLowerCase();

            // Get all rows from the instructor table
            var table = document.getElementById("instructor-table");
            var rows = table.getElementsByTagName("tr");

            // Loop through all table rows and hide those that don't match the search query
            for (var i = 1; i < rows.length; i++) {
                var td = rows[i].getElementsByTagName("td")[0]; // Get the 'Name' column (index 0)
                if (td) {
                    var txtValue = td.textContent || td.innerText;
                    if (txtValue.toLowerCase().indexOf(filter) > -1) {
                        rows[i].style.display = "";
                    } else {
                        rows[i].style.display = "none";
                    }
                }
            }
        }
    </script>
</body>

</html>
