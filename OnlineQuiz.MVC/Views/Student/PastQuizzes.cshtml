<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>QuizTech | Dashboard</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
        :root {
            --primary-color: #391560;
            --secondary: #b12166;
            --action: #f77f00;
            --tertiary: #f0f3f8;
            --headingcolor: rgb(7, 10, 17);
        }

        body {
            background-color: var(--tertiary);
            color: var(--headingcolor);
            font-family: 'Arial', sans-serif;
        }

        .card {
            border: none;
            border-radius: 15px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        }

        .card-header {
            background-color: var(--primary-color);
            color: white;
            border-top-left-radius: 15px;
            border-top-right-radius: 15px;
            padding: 1rem;
        }

        .btn-primary {
            background-color: var(--action);
            border-color: var(--action);
        }

            .btn-primary:hover {
                background-color: var(--secondary);
                border-color: var(--secondary);
            }

        .table th {
            color: var(--headingcolor);
        }

        .table-hover tbody tr:hover {
            background-color: rgba(255, 255, 255, 0.1);
        }

        .completion-rate {
            font-weight: bold;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="#">QuizTech</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Dashboard</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Quizzes</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Profile</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Logout</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container mt-5">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h2 class="mb-0">Past Quizzes</h2>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>No.</th>
                                        <th>Quiz Title</th>
                                        <th>Subject</th>
                                        <th>Due Date</th>
                                        <th>Completion Rate</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody id="quizTableBody">
                                    <!-- Data will be dynamically inserted here using JavaScript -->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script>
        // Example of JavaScript data insertion into the table
        const completionRates = [
            {
                quiz: { id: 1, title: 'Quiz 1', subject: 'Math', due_datetime: '2023-10-10 12:00:00' },
                completionRate: 85
            },
            {
                quiz: { id: 2, title: 'Quiz 2', subject: 'Science', due_datetime: '2023-10-15 14:00:00' },
                completionRate: 90
            }
        ];

        const quizTableBody = document.getElementById('quizTableBody');

        completionRates.forEach((quizData, index) => {
            const row = document.createElement('tr');
            row.innerHTML = `
                        <td>${index + 1}</td>
                        <td>${quizData.quiz.title}</td>
                        <td>${quizData.quiz.subject}</td>
                        <td>${new Date(quizData.quiz.due_datetime).toLocaleString()}</td>
                        <td class="completion-rate">${quizData.completionRate}%</td>
                        <td>
                            <button class="btn btn-sm btn-primary" onclick="showQuizDetails(${quizData.quiz.id})">
                                <i class="fas fa-info-circle"></i> Details
                            </button>
                        </td>
                    `;
            quizTableBody.appendChild(row);
        });

        function showQuizDetails(quizId) {
            alert('Show details for quiz ID: ' + quizId);
        }
    </script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.10.2/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>
